{
  "name": "bifrost",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "npm run migrate-up",
    "start": "node ./build/bin/www",
    "prestart-local": "NODE_ENV=local npm run migrate-up && NODE_ENV=local npm run insert-fixtures",
    "start-local": "NODE_ENV=local nodemon ./bin/www",
    "build": "rimraf ./build && tsc",
    "test": "jest",
    "integration-test": "jest -c jest.config.integration.js",
    "test-all": "npm run test && npm run integration-test",
    "setup-local-db": "NODE_ENV=local db-migrate db:drop bifrost; db-migrate db:create bifrost && npm run migrate-up",
    "prestart-docker-compose": "NODE_ENV=dockerlocal npm run migrate-up && NODE_ENV=dockerlocal npm run insert-fixtures",
    "start-docker-compose": "NODE_ENV=dockerlocal nodemon ./bin/www",
    "migrate-create": "db-migrate create",
    "migrate-up": "dotenv -e ./config/${NODE_ENV}.env db-migrate up",
    "migrate-down": "dotenv -e ./config/${NODE_ENV}.env db-migrate down",
    "insert-fixtures": "jest --detectOpenHandles --forceExit -c jest.config.fixture.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "axios-logger": "^2.3.0",
    "body-parser": "^1.19.0",
    "cldr": "^5.6.0",
    "cldr-data": "^36.0.0",
    "cors": "^2.8.5",
    "crypto-js": "^3.1.9-1",
    "dateformat": "^3.0.3",
    "db-migrate": "^0.11.9",
    "db-migrate-pg": "^1.0.0",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^3.1.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-basic-auth": "^1.2.0",
    "express-healthcheck": "^0.1.0",
    "express-jsonschema": "^1.1.6",
    "express-locale": "^2.0.0",
    "express-mung": "^0.5.1",
    "express-useragent": "^1.0.13",
    "express-winston": "^4.0.3",
    "form-data": "^3.0.0",
    "generate-password": "^1.5.1",
    "globalize": "^1.5.0",
    "heroku-ssl-redirect": "0.0.4",
    "highlight.js": "^10.0.3",
    "hot-shots": "^7.4.2",
    "html-entities": "^1.3.1",
    "http-errors": "~1.6.3",
    "is-base64": "^1.0.0",
    "is-uuid": "^1.0.2",
    "lodash": "^4.17.20",
    "mailgun-js": "^0.22.0",
    "markdown-it": "^10.0.0",
    "moment": "^2.24.0",
    "morgan": "~1.9.1",
    "node-cache": "^5.0.0",
    "pg": "^8.5.1",
    "pug": "^2.0.4",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "testcontainers": "^4.7.0",
    "uuid": "^3.3.3",
    "winston": "^3.2.1",
    "xmlbuilder2": "^2.1.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.0.1",
    "@types/express": "^4.17.8",
    "@types/is-base64": "^1.1.0",
    "@types/is-uuid": "^1.0.0",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.162",
    "@types/request": "^2.48.5",
    "eslint": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.6.1",
    "jest-html-reporters": "^1.2.1",
    "jest-junit": "^10.0.0",
    "nock": "^12.0.3",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
